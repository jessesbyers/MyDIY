<h1> Add a Resource for this Goal </h1>

<%= render partial: "layouts/errors", locals: {object: @resource} %>

<%= form_for(@resource, url: goal_resources_path) do |f| %>
    <%= f.hidden_field :id %>

    <%= f.label :url %>
    <%= f.url_field :url %><br>

    <%= f.label :description %>
    <%= f.text_area :description %><br>

    <%= f.submit nil, class: 'btn btn-secondary' %>
<% end %>


